<h1>Listing binaires</h1>

<%= show_pages_links(@binaire_pages, 'une archive binaire') %>

<% titres = ['Id','Patch']%>
<%= show_table(@binaires, Binaire, titres, :content_columns => true) { |b|
  result = ''
  result << "<td>#{b.id}</td>"
  result << "<td>#{link_to_file(b, 'fichier')}</td>"
  result << "#{link_to_actions_table b}"
}%>


