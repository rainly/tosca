<h1><%= t(:Index)%></h1>

<% paginated_section(@tags, :create_label => t('a skill')) do %>
 <% titles = [ t(:Name), t(:'Tag.Submitter'), t(:'Models.Skill'), t(:'Models.Contract')]%>
 <%= show_table(@tags, Tag, titles) do |t|
   result = ''
   result << "<td>#{t.name}</td>"
   result << "<td>#{t.user}</td>"
   result << "<td>#{t.skill}</td>"
   if t.contract.nil?
     result << "<td>#{t.contract}</td>"
   else
     result << "<td>#{t.contract.name}</td>"
   end
   result << "#{link_to_actions_table t}"
 end %><br />
 <%= page_entries_info @tags %>
<% end %>
